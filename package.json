{
  "name": "expression",
  "version": "1.0.0",
  "description": "expression evaluator",
  "main": "lib/expression.js",
  "scripts": {
    "cover": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports test",
