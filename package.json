{
  "name": "expression-expander",
  "version": "0.0.0-semantic-release",
  "description": "expands expressions in object graphs",
  "main": "dist/expander.js",
  "module": "src/expander.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup -c -- src/expander.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepublish && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/expression-expander.git"
  },
  "engines": {
    "node": ">=8.1.2"
  },
  "keywords": [
    "expression",
    "property",
    "evaluate"
  ],
  "release": {
    "verifyRelease": "cracks"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "cracks": "3.1.2",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "semantic-release": "^6.3.6",
    "xo": "^0.19.0"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/expression-expander/issues"
  },
  "homepage": "https://github.com/arlac77/expression-expander#readme",
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "babel": {
      "presets": []
    },
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
