{
  "name": "expression-expander",
  "version": "0.0.0-semantic-release",
  "description": "expands expressions in object graphs",
  "main": "dist/expander.js",
  "jsnext:main": "src/expander.js",
  "module": "src/expander.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/expander.js -c -- src/expander.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/expression-expander.git"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "keywords": [
    "expression",
    "property",
    "evaluate"
  ],
  "release": {
    "verifyRelease": "cracks"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rollup": "^0.41.4",
    "semantic-release": "^6.3.6"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/expression-expander/issues"
  },
  "homepage": "https://github.com/arlac77/expression-expander#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}