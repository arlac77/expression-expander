{
  "name": "expression-expander",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/expander.js",
  "module": "src/expander.js",
  "description": "expands expressions in object graphs",
  "keywords": [
    "expression",
    "property",
    "evaluate"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/expander.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/expander.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.6.3",
    "documentation": "^8.0.0",
    "travis-deploy-once": "^5.0.1"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/expression-expander.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/expression-expander/issues"
  },
  "homepage": "https://github.com/arlac77/expression-expander#readme",
  "ava": {
    "babel": {
      "presets": []
    },
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xrelease": {
    "verifyRelease": [
      "cracks",
      {
        "path": "dont-crack",
        "test-against": [
          "https://github.com/arlac77/config-expander"
        ]
      }
    ]
  }
}
